{
  "type": "script",
  "seleniumVersion": "2",
  "formatVersion": 1,
  "steps": [
    {
      "type": "get",
      "url": "https://ignite.where2stageit.com/login.html"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "loginid"
      },
      "text": "qa_tester"
    },
    {
      "type": "setElementText",
      "locator": {
        "type": "id",
        "value": "password"
      },
      "text": "Where2getit!"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "submit"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "css selector",
        "value": "img[alt=\"Reputation\"]"
      }
    },
    {
      "type": "waitForTextPresent",
      "text": "Google+ Local"
    },
    {
      "type": "waitForTextPresent",
      "text": "Yelp"
    },
    {
      "type": "verifyText",
      "locator": {
        "type": "id",
        "value": ""
      },
      "text": "Yahoo Local"
    },
    {
      "type": "verifyText",
      "locator": {
        "type": "id",
        "value": ""
      },
      "text": "Foursquare"
    },
    {
      "type": "verifyText",
      "locator": {
        "type": "id",
        "value": ""
      },
      "text": "Facebook"
    },
    {
      "type": "verifyTextPresent",
      "text": "Reviewer"
    },
    {
      "type": "verifyTextPresent",
      "text": "Location Name"
    },
    {
      "type": "verifyTextPresent",
      "text": "City & State"
    },
    {
      "type": "verifyTextPresent",
      "text": "Review"
    },
    {
      "type": "verifyTextPresent",
      "text": "Rating"
    },
    {
      "type": "verifyTextPresent",
      "text": "Review Date"
    },
    {
      "type": "verifyTextPresent",
      "text": "We did not find any review data"
    },
    {
      "type": "verifyTextPresent",
      "text": "Next 25"
    },
    {
      "type": "verifyTextPresent",
      "text": "Last 25"
    },
    {
      "type": "verifyText",
      "locator": {
        "type": "id",
        "value": ""
      },
      "text": "Show 25 50 100 results per page"
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Back to Launchpad"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "id",
        "value": "logout-button"
      }
    },
    {
      "type": "clickElement",
      "locator": {
        "type": "link text",
        "value": "Back to Log In"
      }
    }
  ]
}